<script> //Lazy loading of images to increase page loading speed and reduce computer resource usage
function putPics(){
    $(function() {
        $('.lazy').lazy();
    });
}
function putLowerPics(){
	$(function() {
        $('.lower_exercises').lazy();
    });
}
function putUpperPics(){
	$(function() {
        $('.upper_exercises').lazy();
    });
}
